{
  "name": "dive",
  "private": true,
  "version": "0.8.1",
  "type": "module",
  "main": "dist-electron/main/index.js",
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenAgentPlatform/Dive"
  },
  "scripts": {
    "dev": "vite --config vite.config.electron.ts",
    "lint": "eslint .",
    "preview": "vite preview",
    "check": "tsc -b",
    "check-i18n": "node scripts/checkI18n.js",
    "build": "tsc -b && vite build --config vite.config.electron.ts",
    "postinstall": "patch-package && electron-builder install-app-deps && vite-node scripts/download-host-deps.ts",
    "docker:build-win": "scripts/docker/build-win.sh",
    "download:windows-bin": "vite-node scripts/download-node.ts win-x64 && vite-node scripts/download-uv.ts win32 x64 && vite-node scripts/download-python-via-uv.ts win-x64",
    "download:darwin-bin": "run-s download:darwin-bin:*",
    "download:darwin-bin:x64": "vite-node scripts/download-node.ts darwin-x64 && vite-node scripts/download-uv.ts darwin x64 && vite-node scripts/download-python-via-uv.ts darwin-x64",
    "download:darwin-bin:arm64": "vite-node scripts/download-node.ts darwin-arm64 && vite-node scripts/download-uv.ts darwin arm64 && vite-node scripts/download-python-via-uv.ts darwin-arm64",
    "download:linux-bin": "vite-node scripts/download-node.ts linux-x64 && vite-node scripts/download-uv.ts linux x64 && vite-node scripts/download-python-via-uv.ts linux-x64",
    "package": "npm run build && electron-builder",
    "package:windows": "npm run download:windows-bin && npm run prepare-python win-x64 && run-s package:windows-*",
    "package:windows-installers": "npm run build && electron-builder --win nsis --x64 --publish=never",
    "package:darwin": "npm run download:darwin-bin && npm run prepare-python darwin && npm run build && electron-builder --mac --arm64 --x64 --publish=never",
    "package:darwin-dmg:arm64": "npm run prepare-python darwin-arm64 && npm run build && electron-builder --mac dmg --arm64 --publish=never",
    "package:darwin-dmg:x64": "npm run prepare-python darwin-x64 && npm run build && electron-builder --mac dmg --x64 --publish=never",
    "package:linux": "npm run download:linux-bin && npm run prepare-python linux-x64 && run-s package:linux-*",
    "package:linux-tar": "npm run build && electron-builder --linux tar.gz --x64 --publish=never",
    "package:linux-appImage": "npm run build && electron-builder --linux appimage --x64 --publish=never",
    "prepare-python": "vite-node scripts/before-package.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@aws-sdk/client-bedrock": "^3.791.0",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lint": "^6.8.4",
    "@mistralai/mistralai": "^1.5.1",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-tooltip": "^1.1.7",
    "@uiw/react-codemirror": "^4.23.8",
    "axios": "^1.7.9",
    "better-sqlite3": "^11.8.1",
    "clsx": "^2.1.1",
    "cross-spawn": "^7.0.6",
    "electron-log": "^5.2.4",
    "electron-store": "^10.0.1",
    "electron-updater": "^6.3.9",
    "env-paths": "^3.0.0",
    "fs-extra": "^11.3.0",
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.0.2",
    "i18next-http-backend": "^3.0.2",
    "jotai": "^2.11.0",
    "jsonlint-mod": "^1.7.6",
    "katex": "^0.16.21",
    "lodash": "^4.17.21",
    "marked": "^15.0.7",
    "marked-react": "^3.0.0",
    "mermaid": "^11.4.1",
    "mitt": "^3.0.1",
    "ollama": "^0.5.15",
    "openai": "^4.95.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.4.0",
    "react-intersection-observer": "^9.16.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.28.1",
    "react-syntax-highlighter": "^15.6.1",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "semver": "^7.7.1",
    "sharp": "^0.33.5",
    "ts-node": "^10.9.2",
    "winston": "^3.17.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/cors": "^2.8.17",
    "@types/cross-spawn": "^6.0.6",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/katex": "^0.16.7",
    "@types/lodash": "^4.17.15",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/tar": "^6.1.13",
    "@types/unzipper": "^0.10.10",
    "@vitejs/plugin-react": "^4.3.4",
    "electron": "^33.4.9",
    "electron-builder": "^25.1.8",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^8.0.0",
    "rimraf": "^6.0.1",
    "sass-embedded": "^1.83.0",
    "ts-jest": "^29.2.5",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "unzipper": "^0.12.3",
    "vite": "^6.1.0",
    "vite-node": "^3.0.1",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vitest": "^2.1.9"
  }
}
